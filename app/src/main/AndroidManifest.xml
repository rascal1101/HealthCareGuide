<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.healthcareguide">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SignupActivity" />
        <activity android:name=".Activity.CarelistForDoctorActivity" />
        <activity android:name=".Activity.CarelistActivity" />
        <activity android:name=".Activity.UserStateGraphActivity" />
        <activity android:name=".Activity.Diabets_Activity" />
        <activity android:name=".Activity.Select_Activity" />
        <activity android:name=".Activity.SymptomActivity"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".Activity.BloodPressureActivity" />
    </application>

</manifest>